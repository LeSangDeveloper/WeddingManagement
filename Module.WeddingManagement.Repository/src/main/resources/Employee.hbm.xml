<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Module.WeddingManagement.ApplicationModel.Employee" table="employee">
        <meta attribute="class-description">Employee Info</meta>
        <id column="id" name="id" type="int"/>
        <property column="title" name="title" type="string"/>
        <property column="fullname" name="fullName" type="string"/>
        <property column="username" name="username" type="string"/>
        <property column="password" name="password" type="string"/>
    </class>
</hibernate-mapping>