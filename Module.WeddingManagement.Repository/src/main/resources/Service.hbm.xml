<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Module.WeddingManagement.ApplicationModel.Service" table="service">
        <meta attribute="class-description">Service Info</meta>
        <id column="id" name="id" type="int"/>
        <property column="price" name="price" type="big_decimal"/>
        <property column="note" name="note" type="string"/>
        <property column="type" name="type" type="string"/>
    </class>
</hibernate-mapping>